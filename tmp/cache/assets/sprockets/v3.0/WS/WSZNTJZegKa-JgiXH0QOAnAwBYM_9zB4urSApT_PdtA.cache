I"n(function() {
  jQuery(function() {
    var departments;
    $("#f").submit(function() {
      var valDDL;
      valDDL = $("#item_name").val();
      if (valDDL === "Choose a Property") {
        event.preventDefault();
        return alert("Select Property option");
      }
    });
    $('#item_department').parent().hide();
    departments = $('#item_department').html();
    return $('#properties_select').parent().change(function() {
      var options, property;
      property = $('#properties_select :selected').text();
      options = $(departments).filter("optgroup[label='" + property + "']").html();
      if (options) {
        $('#item_department').html(options);
        return $('#item_department').parent().show();
      } else {
        $('#item_department').empty();
        return $('#item_department').parent().hide();
      }
    });
  });

}).call(this);
